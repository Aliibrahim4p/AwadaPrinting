# Force GitHub to treat files correctly
*.php linguist-language=PHP
*.js linguist-language=JavaScript
*.css linguist-language=CSS
*.html linguist-language=HTML
.tsx linguist-language=TypeScriptX
.jsx linguist-language=JavaScriptX
.ts linguist-language=TypeScript
# Ignore vendor / node_modules
vendor/* linguist-vendored
node_modules/* linguist-vendored
# Mark a single file as vendored (ignored by GitHub language stats)
awadapriting-api/config/database.php linguist-vendored
